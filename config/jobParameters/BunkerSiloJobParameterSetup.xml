<?xml version="1.0" encoding="UTF-8"?>

<!-- 
	Bale finder ai parameters.
	For documentation look up the CpSettingUtil.lua.
-->

<Settings prefixText="CP_bunkerSiloJobParameters_">
	<SettingSubTitle title="ai_parameterGroupTitlePosition" prefix="false">
		<Setting classType="CpAIParameterPositionAngle" name="startPosition" positionParameterType="DRIVE_TO"/>
	</SettingSubTitle>
	<SettingSubTitle title="CP_fieldWorkJobParameters_bunkerSiloPosition_title" prefix="false">
		<Setting classType="CpAIParameterPosition" name="siloPosition" positionParameterType="FIELD_OR_SILO"/>
	</SettingSubTitle>
	<SettingSubTitle title="drivingForwardsIntoSilo">
		<Setting classType="AIParameterBooleanSetting" name="drivingForwardsIntoSilo" defaultBool="true" isVisible="isDrivingForwardsIntoSiloSettingVisible" isDisabled="isCpActive">
			<Texts>
				<Text>reverse</Text>
				<Text>forwards</Text>
			</Texts>
		</Setting>
	</SettingSubTitle>
	<SettingSubTitle title="waitAtParkPosition">
		<Setting classType="AIParameterBooleanSetting" name="waitAtParkPosition" defaultBool="false" isDisabled="isCpActive">
			<Texts>
				<Text>silo</Text>
				<Text>parkPosition</Text>
			</Texts>
		</Setting>
	</SettingSubTitle>
	<SettingSubTitle title="stopWithCompactedSilo">
		<Setting classType="AIParameterBooleanSetting" name="stopWithCompactedSilo" defaultBool="false" isDisabled="isCpActive"/>
	</SettingSubTitle>
</Settings>
